<h3>Your Posts</h3>

{{#each posts}}
<div class="card">
    <div class="card-body">
        <h5 class="card-title">
            <a href="/post/{{id}}">{{title}}</a>
        </h5>
        <h6 class="card-subtitle mb-2 text-muted">Posted on {{format_date dateCreated}}</h6>
        <p class="card-text">{{content}}</p>
    </div>
</div>
{{/each}}